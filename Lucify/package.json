    "compression": "1.7.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.6",
    "dotenv": "^6.2.0",
    "express": "4.16.3",
    "highcharts": "^7.0.3",
    "highcharts-react-official": "^2.0.0",
    "ip": "1.1.5",
    "lodash": "^4.17.10",
    "minimist": "1.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.3.15",
    "prop-types": "15.6.1",
    "react": "16.5.2",
    "react-countup": "^4.0.0-alpha.6",
    "react-d3-cloud": "^0.7.0",
    "react-dom": "^16.4.0",
    "react-helmet": "5.2.0",
    "react-moment": "^0.8.4",
    "react-places-autocomplete": "^7.2.0",
    "react-pose": "^3.3.7",
    "react-redux": "5.0.7",
